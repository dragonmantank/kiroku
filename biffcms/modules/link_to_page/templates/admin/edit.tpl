<h2>Edit '{page_name}'</h2>

<form method="Post">
<table>
	<tr>
		<td>Link Name:</td>
		<td><input type="text" name="name" value="{name}"></td>
	</tr>
	<tr>
		<td>URL:</td>
		<td><input type="text" name="url" value="{url}" size="50"></td>
	</tr>
	<tr>
		<td align="right">
		<input type="submit" value="Save & Exit" name="save_exit"> 
		<input type="submit" value="Cancel" name="cancel"></td>
	</tr>
</table>
</form>
